{
    "compilerOptions": {
        "baseUrl": ".",
        "moduleResolution": "node",
        "outDir": "js",
        "paths": {
            "ws": ["./js/ws-es6/index.js"]
        },
        "pretty": true,
        "target": "ES6",
        "types": ["node"],
        "typeRoots": ["node_modules/@types"]
    },
    "exclude": [
        "js",
        "node_modules"
    ],
    "include": [
        "*.ts",
        "**/*.ts"
    ]
}